<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".set.SetActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="集合中不包含重复元素"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/arraySet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ArraySet"
        android:textAllCaps="false"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/hashSet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="HashSet"
        android:textAllCaps="false"
        app:layout_constraintTop_toBottomOf="@+id/arraySet"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/linkedHashSet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="LinkedHashSet"
        android:textAllCaps="false"
        app:layout_constraintTop_toBottomOf="@+id/hashSet"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/enumSet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="EnumSet"
        android:textAllCaps="false"
        app:layout_constraintTop_toBottomOf="@+id/linkedHashSet"
        tools:layout_editor_absoluteX="16dp" />


    <Button
        android:id="@+id/treeSet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TreeSet"
        android:textAllCaps="false"
        app:layout_constraintTop_toBottomOf="@+id/enumSet"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/copyOnWriteArraySet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CopyOnWriteArraySet"
        android:textAllCaps="false"
        app:layout_constraintTop_toBottomOf="@+id/treeSet"
        tools:layout_editor_absoluteX="-16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>